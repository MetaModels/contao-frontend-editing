{
  "name": "metamodels/contao-frontend-editing",
  "description": "MetaModels frontend editing integrations",
  "keywords": [
    "contao",
    "metamodels",
    "frontend-editing"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/contao-frontend-editing/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/contao-frontend-editing"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "contao-community-alliance/composer-plugin": "^2.4",
    "contao-community-alliance/dc-general": "^2.0.0-beta49@beta",
    "contao-community-alliance/dc-general-contao-frontend": "~2.0@dev",
    "contao-community-alliance/dependency-container": "~1.6",
    "contao-community-alliance/event-dispatcher": "~1.3",
    "contao-community-alliance/events-contao-bindings": "~3.2",
    "contao-community-alliance/url-builder": "~1.1",
    "contao/core": "^3.5.5",
    "metamodels/core": "^2.0"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "~0.6",
    "metamodels/base-unit-tests": "~1.0",
    "metamodels/phpunit-contao-database": "~1.0",
    "phpcq/all-tasks": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\Contao\\FrontendEditing\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\Test\\Contao\\FrontendEditing\\": "tests"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/metamodels-contao-frontend-editing"
      },
      "transifex": {
        "project": "metamodels",
        "prefix": "contao-frontend-editing-",
        "languages_cto": "contao/languages",
        "languages_tx": ".tx"
      },
      "runonce": [
        "runonce/runonce.php"
      ]
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
